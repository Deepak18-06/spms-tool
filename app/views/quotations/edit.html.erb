<div class="container">
  <h1>Editing quotation</h1>

  <%= render "form", quotation: @quotation %>
  <hr>
  <div class="row">
    <div class="col-md-2">
      <%= link_to "Show this quotation", @quotation, class: "btn btn-primary btn-block" %>
    </div>
    <div class="col-md-2">
      <%= link_to "Back to quotations", quotations_path, class: "btn btn-success btn-block" %>
    </div>
  </div>
</div>

